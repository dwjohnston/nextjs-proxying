{
  "name": "nextjs-proxying",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:secure": "NODE_EXTRA_CA_CERTS=~/.mitmproxy/mitmproxy-ca-cert.pem next dev",
    "proxy": "NODE_EXTRA_CA_CERTS=~/.mitmproxy/mitmproxy-ca-cert.pem node proxy.js",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "file": "node file.mjs",
    "file:secure": "NODE_EXTRA_CA_CERTS=~/.mitmproxy/mitmproxy-ca-cert.pem node file.mjs", 
    "mitm": "mitmproxy", 
    "mitm:replay": "mitmproxy --set server_replay=1.har --set server_replay_reuse=true"
  },
  "dependencies": {
    "http-proxy": "^1.18.1",
    "https-proxy-agent": "^7.0.5",
    "next": "14.2.5",
    "node-fetch": "^3.3.2",
    "react": "^18",
    "react-dom": "^18",
    "undici": "^6.19.7"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.2.5",
    "typescript": "^5"
  }
}
